<p id="notice"><%= notice %></p>

<div class="row">
 
 <div class="slider">
    <ul class="slides">
    <% @events.each do |event| %>
      <li>
      
      <%= link_to event do %>
        <%= image_tag (event.avatar_url) %> <!-- random image -->
      <% end %>
      </li>
      <% end %>
    </ul>
  </div>
</div>

  
     

