<% @jobs.each do |job| %>
  <div class="col s12 m6 l3">  
  <%= link_to job do %> 
      <div class="card small">
        <div class="card-image image">
        <%= image_tag job.user.image %> 
        </div> 
        <div class="card-content">
          <span class="sub2 activator grey-text text-darken-4"><%= job.title %></span>
          <p class=""><%= job.sector.title %></p>
        </div>
        <div class="card-action center">
            <span class="blue-text text-darken-2">maringa </span> - 
            <span class="blue-text text-darken-2"> <%= time_ago_in_words(job.created_at) %></span>
        </div>
         
      </div> 
      <% end %>  
  </div>
  <% end %>